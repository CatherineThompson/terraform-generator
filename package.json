{
  "name": "terraform-generator",
  "version": "0.2.3",
  "author": "Ah Zhe",
  "description": "Generate Terraform plan using Node.js.",
  "keywords": [
    "Terraform"
  ],
  "files": [
    "dist/**/*"
  ],
  "homepage": "https://github.com/ahzhezhe/terraform-generator#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/ahzhezhe/terraform-generator"
  },
  "license": "ISC",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint src/**/*.ts test/**/*.ts",
    "test": "jest",
    "tryout": "ts-node tryout.ts",
    "build": "tsc",
    "prepublishOnly": "del-cli --force dist && tsc",
    "postpublish": "del-cli --force dist"
  },
  "devDependencies": {
    "@types/jest": "^25.1.1",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "del-cli": "^3.0.0",
    "eslint": "^5.16.0",
    "eslint-plugin-import": "^2.17.2",
    "jest": "^25.1.0",
    "ts-jest": "^25.1.0",
    "ts-node": "^7.0.0",
    "typescript": "^3.4.2"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "test\\.ts$",
    "collectCoverage": true,
    "coverageDirectory": "./test/__coverage__",
    "coveragePathIgnorePatterns": [
      "node_modules",
      "test"
    ]
  }
}
