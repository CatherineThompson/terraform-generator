{
  "name": "terraform-generator",
  "version": "1.0.1",
  "author": "Ah Zhe",
  "description": "Generate Terraform plan using Node.js.",
  "keywords": [
    "Terraform"
  ],
  "homepage": "https://github.com/ahzhezhe/terraform-generator#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/ahzhezhe/terraform-generator"
  },
  "license": "ISC",
  "files": [
    "dist/**/*"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint src/**/*.ts test/**/*.ts",
    "lint-mac": "eslint 'src/**/*.ts' 'test/**/*.ts'",
    "test": "jest",
    "tryout": "ts-node tryout.ts",
    "build": "del-cli --force dist && tsc",
    "prepublishOnly": "npm run build",
    "postpublish": "del-cli --force dist"
  },
  "devDependencies": {
    "@types/jest": "^25.1.3",
    "@types/shelljs": "^0.8.6",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "del-cli": "^3.0.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "^25.1.0",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.2"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "test\\.ts$",
    "collectCoverage": true,
    "coverageDirectory": "./test/__coverage__",
    "coveragePathIgnorePatterns": [
      "node_modules",
      "test"
    ]
  },
  "dependencies": {
    "shelljs": "^0.8.3"
  }
}
